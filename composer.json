{
  "name": "fournet/imagedb-bundle",
  "type": "contao-bundle",
  "description": "Contao 4 bundle for the Image-DB",
  "license": "proprietary",
  "authors": [
    {
      "name": "Janosch Oltmanns"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "contao/core-bundle": ">=4.3"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "Fournet\\ImagedbBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Fournet\\ImagedbBundle\\ContaoManager\\Plugin"
  }
}
